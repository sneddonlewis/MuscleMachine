<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity TrainingSet.
    -->
    <changeSet id="20230204111502-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="resistance_id"
                                 baseTableName="training_set"
                                 constraintName="fk_training_set__resistance_id"
                                 referencedColumnNames="id"
                                 referencedTableName="resistance"/>

        <addForeignKeyConstraint baseColumnNames="strength_workout_id"
                                 baseTableName="training_set"
                                 constraintName="fk_training_set__strength_workout_id"
                                 referencedColumnNames="id"
                                 referencedTableName="strength_workout"/>
    </changeSet>
</databaseChangeLog>
